{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:custom-rule": "pnpm custom-rule:tsc && pnpm custom-rule:format",
    "dev": "nuxt dev -o",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rules": "npx @eslint/config-inspector@latest",
    "custom-rule:tsc": "tsc --project settings/rules/tsconfig.json",
    "custom-rule:format": "prettier --write settings/rules/dist/**",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint-and-format": "pnpm lint:fix && pnpm format:fix",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "convert": "npx vite-node settings/data/convert.ts",
    "ci:typecheck": "npx nuxi typecheck .",
    "ci:test": "vitest --coverage",
    "ci:lint": "eslint .",
    "ci:format": "prettier --check .",
    "predev": "pnpm convert",
    "prepare": "husky"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "3.1.0",
    "@pinia/nuxt": "0.10.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-vue-next": "0.475.0",
    "nuxt": "3.15.4",
    "pinia": "3.0.1",
    "radix-vue": "1.9.14",
    "shadcn-nuxt": "0.11.3",
    "tailwind-merge": "3.0.1",
    "tailwindcss-animate": "1.0.7",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@nuxt/eslint": "1.0.1",
    "@nuxt/test-utils": "3.15.4",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/tailwindcss": "6.13.1",
    "@pinia/testing": "1.0.0",
    "@types/eslint": "9.6.1",
    "@types/estree": "1.0.6",
    "@types/node": "22.13.4",
    "@typescript-eslint/parser": "8.24.0",
    "@vitest/coverage-v8": "3.0.5",
    "@vitest/ui": "3.0.5",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/test-utils": "2.4.6",
    "commitizen": "4.3.1",
    "commitlint-config-gitmoji": "2.3.1",
    "cz-customizable": "7.4.0",
    "eslint": "9.20.1",
    "eslint-plugin-reactive-value-suffix": "2.0.0",
    "glob": "11.0.1",
    "globals": "15.15.0",
    "happy-dom": "16.8.1",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "playwright-core": "1.50.1",
    "prettier": "3.5.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "5.7.3",
    "vite-node": "3.0.5",
    "vitest": "3.0.5",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.1.10"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,cts,mts,vue}": [
      "pnpm convert",
      "eslint --fix"
    ],
    "**/*.{js,mjs,cjs,ts,cts,mts,vue,json}": "prettier --write",
    "**/*.{ts,cts,mts,vue}": "nuxi typecheck ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "9.15.5"
  }
}
