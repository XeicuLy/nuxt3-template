{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev -o",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rules": "npx @eslint/config-inspector@latest",
    "format": "prettier --write .",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "nuxt": "^3.11.2",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "eslint": "^9.3.0",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript": "^5.4.5",
    "vite-plugin-eslint2": "^4.4.0",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": "npx eslint --fix",
    "**/*.{js,ts,vue,json}": "npx prettier --write",
    "**/*.{ts,vue}": "npx vue-tsc --noEmit"
  },
  "volta": {
    "node": "20.13.1",
    "pnpm": "9.1.2"
  }
}
