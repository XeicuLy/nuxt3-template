{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:custom-lint": "tsc --project settings/rules/tsconfig.json",
    "dev": "nuxt dev -o",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rules": "npx @eslint/config-inspector@latest",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint-and-format": "pnpm lint:fix && pnpm format:fix",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "convert": "npx vite-node settings/data/convert.ts",
    "ci:typecheck": "npx nuxi typecheck .",
    "ci:test": "vitest --coverage",
    "ci:lint": "eslint .",
    "ci:format": "prettier --check .",
    "predev": "pnpm convert",
    "prepare": "husky"
  },
  "dependencies": {
    "@pinia/nuxt": "0.7.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lucide-vue-next": "0.460.0",
    "nuxt": "3.13.2",
    "pinia": "2.2.6",
    "radix-vue": "1.9.9",
    "shadcn-nuxt": "0.10.4",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "1.0.7",
    "vue": "3.5.13",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@nuxt/eslint": "0.7.0",
    "@nuxt/test-utils": "3.14.4",
    "@nuxtjs/tailwindcss": "6.12.2",
    "@pinia/testing": "0.1.7",
    "@types/eslint": "9.6.1",
    "@types/estree": "1.0.6",
    "@types/node": "22.9.0",
    "@typescript-eslint/parser": "8.14.0",
    "@vitest/coverage-v8": "2.1.5",
    "@vitest/ui": "2.1.5",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/test-utils": "2.4.6",
    "commitizen": "4.3.1",
    "commitlint-config-gitmoji": "2.3.1",
    "cz-customizable": "7.2.1",
    "eslint": "9.14.0",
    "eslint-plugin-reactive-value-suffix": "2.0.0",
    "glob": "11.0.0",
    "globals": "15.12.0",
    "happy-dom": "15.11.6",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "playwright-core": "1.48.2",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "typescript": "5.6.3",
    "vite-node": "2.1.5",
    "vitest": "2.1.5",
    "vue-tsc": "2.1.10"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,cts,mts,vue}": [
      "pnpm convert",
      "eslint --fix"
    ],
    "**/*.{js,mjs,cjs,ts,cts,mts,vue,json}": "prettier --write",
    "**/*.{ts,cts,mts,vue}": "nuxi typecheck ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "9.13.2"
  }
}
